#include<iostream>
#include<string>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string s;
    cin>>s;
    int counter1=0, counter2 = 0;

    for(int i = 0 ; i< n; i++)
    {
        if(s[i]=='D')
        {
            counter1++;
        }
        else if(s[i]=='A')
        {
            counter2 ++;
        }
    }
    if(counter1>counter2)
    {
        cout<<"Danik";
    }
    else if(counter2>counter1)
    {
        cout<<"Anton";
    }
    else
    {
        cout<<"Friendship";
    }

    return 0;
}
